t={handleFormSubmit}>\n          <div>\n            {/* Auto fill with user's name */}\n            <label className=\"p-2\">Name:</label>\n            <input\n              type=\"text\"\n              name=\"name\"\n              onChange={handleChange}\n              value={formState.name}\n            />\n          </div>\n          <div>\n            <label className=\"p-2\">Hours Worked:</label>\n            <input\n              type=\"number\"\n              name=\"hours_worked\"\n              onChange={handleChange}\n              value={formState.hours_worked}\n            />\n          </div>\n          <div>\n            <label className=\"p-2\">Role:</label>\n            <input\n              type=\"text\"\n              name=\"role\"\n              onChange={handleChange}\n              value={formState.role}\n            />\n          </div>\n          <div>\n            <label className=\"p-2\">Job Site:</label>\n            <input\n              type=\"text\"\n              name=\"job_site\"\n              onChange={handleChange}\n              value={formState.job_site}\n            />\n          </div>\n          <div>\n            <label className=\"p-2\">Comments:</label>\n            <textarea\n              name=\"comments\"\n              onChange={handleChange}\n              value={formState.comments}\n            />\n          </div>\n          <div>\n            <button type=\"submit\" className=\"btn btn-danger m-1\">\n              Submit Log\n            </button>\n          </div>\n        </form>\n      </div>\n    </div>\n  );\n};\n\nexport default CreateForm;\n","C:\\Users\\wowja\\code\\projects\\JSWorkLogger\\js-work-logger\\client\\src\\pages\\Register.js",[],"C:\\Users\\wowja\\code\\projects\\JSWorkLogger\\js-work-logger\\client\\src\\pages\\Profile.js",[],"C:\\Users\\wowja\\code\\projects\\JSWorkLogger\\js-work-logger\\client\\src\\utils\\auth.js",[],"C:\\Users\\wowja\\code\\projects\\JSWorkLogger\\js-work-logger\\client\\src\